#include <stdio.h>
int BS(int A[], int FirstI, int LastI, int key){
   while (FirstI <= lastI){
      int m = FirstI + (LastI- FirstI)/2;
      if (A[m] == key)
         return m;
      if (A[m] < key)
         FirstI = m + 1;
      else
         LastI = m - 1;
   }
   return -1;
}
int main(void){
   int A[] = {1, 4, 7, 9, 16, 56, 70};
   int n = 7;
   int key = 16;
   int f = BS(A, 0, n-1, key);
   if(f == -1) {
      printf("Key not found ");
   }
   else {
      printf("Key found at index : %d", f);
   }
   return 0;
}
