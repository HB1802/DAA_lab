#include<stdio.h>

int main()
{
    int N;
    int i = 0;
    scanf("%d", &N);
    int A[N];
    printf("Enter the Array\n");
    for(int i = 0; i < N; i++)
    {
        scanf("%d", &A[i]);
    }
    int M;
    printf("Enter the no. to be searched\n");
    scanf("%d", &M);
    for(int i= 0; i < N; i++)
    {
        if(A[i] == M)
        {    
            printf("The key is present\n");
            break;
        }
    }

    return 0;
}
